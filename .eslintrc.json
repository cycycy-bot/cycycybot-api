{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "max-len": 0,
        "consistent-return" : 0,
        "no-param-reassign": 0,
        "array-callback-return": 0
    }
}